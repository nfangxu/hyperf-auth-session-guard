{
    "name": "fx/session-guard",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Fx\\SessionGuard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.5",
        "fx/hyperf-http-auth": "^2.0",
        "fangx/hyperf-cookie": "^2.0",
        "hyperf/session": "~2.0.0",
        "hyperf/utils": "~2.0.0",
        "hyperf/framework": "~2.0.0",
        "hyperf/di": "~2.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpstan/phpstan": "^0.12",
        "hyperf/testing": "~2.0.0",
        "swoole/ide-helper": "^4.5"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Fx\\SessionGuard\\ConfigProvider"
        }
    }
}
